### default for all ###


### override per host ###
Host devstar
	Hostname 10.4.19.92 
	User mike
  AddKeysToAgent yes
  IdentitiesOnly yes
	IdentityFile /home/mike/.ssh/default/default_ed

Host devdell
	Hostname 10.4.19.77 
	User mfurtado
  AddKeysToAgent yes
  IdentitiesOnly yes
	IdentityFile /home/mike/.ssh/default/default_ed
  
# Personal BitBucket account
Host bitbucket.org
    Hostname bitbucket.org
    User git
    AddKeysToAgent yes
    IdentitiesOnly yes
    IdentityFile ~/.ssh/BitBucket/bitbucket_personal_ed

# Work BitBucket account
Host bitbucket.org-work
    Hostname bitbucket.org
    User git
    AddKeysToAgent yes
    IdentitiesOnly yes
    IdentityFile ~/.ssh/BitBucket/bitbucket_work_ed

# Just Add Power Workspace BitBucket Account
Host bitbucket.org-workspace
    Hostname bitbucket.org
    User git
    AddKeysToAgent yes
    IdentitiesOnly yes
    IdentityFile ~/.ssh/BitBucket/bitbucket_workspace_ed

